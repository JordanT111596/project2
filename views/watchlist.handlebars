<div class="container">
  <nav role="navigation">
    <div id="menuToggle">
      <!--
        A fake / hidden checkbox is used as click reciever,
        so you can use the :checked selector on it.
        -->
      <input type="checkbox" />

      <!--
        Some spans to act as a hamburger.
        -->
      <span></span>
      <span></span>
      <span></span>

      <!--
        menu options
        -->
      <ul id="menu">
        <a href="/results">
          <li>Search Results</li>
        </a>
        <a href="/favorites">
          <li>Favorites</li>
        </a>
        <a href="/curate">
          <li>Curate Playlist</li>
        </a>
        <a href="/watched">
          <li>Watched</li>
        </a>
        <a href="/logout">
          <li>Logout</li>
        </a>
      </ul>
    </div>
  </nav>
</div>

<div class="container">
  <div class="col-sm">
    <img class="img-fluid playlistTitle" src="Pandemic-Playlist-Title-Text.png" alt="logo"
      style="max-height: 200px; length: 200px;" />
  </div>
</div>

<div class="container-fluid mt-0" id="moviemargin">
  <div class="row">

    {{#each Movie}}
    {{#if onPlaylist}}
    {{> infoCards/PlaylistCards onPlaylist=true}}
    {{/if}}
    {{/each}}

  </div>
</div>



